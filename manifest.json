{
    "name": "Copyable",
    "description": "A Chrome extension that makes text copyable.",
    "version": "0.1",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "",
            "32": "",
            "48": "",
            "128": ""
        }
    },
    "icons": {
        "16": "",
        "48": "",
        "32": "",
        "128": ""
    }
}